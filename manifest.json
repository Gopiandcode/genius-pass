{
    "manifest_version": 2,
  
    "name": "GeniusPass",
    "description": "Password management for the modern man.",
    "version": "0.1",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "js": ["jquery.js", "main.js"],
            "matches": ["http://*/*", "https://*/*"]
        }
    ],
    "permissions": [
      "activeTab",
      "storage"
    ]
  }